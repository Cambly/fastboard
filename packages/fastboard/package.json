{
  "name": "@netless/fastboard",
  "version": "0.1.1",
  "description": "A tiny wrapper of white-web-sdk and @netless/window-manager.",
  "main": "src/index.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "cleanup": "rimraf dist",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@netless/fastboard-core": "workspace:*",
    "@netless/fastboard-react": "workspace:*"
  },
  "peerDependencies": {
    "@netless/app-slide": "*",
    "@netless/window-manager": ">=0.4.0",
    "white-web-sdk": ">=2.16.0"
  },
  "peerDependenciesMeta": {
    "@netless/app-slide": {
      "optional": true
    }
  },
  "devDependencies": {
    "@netless/app-slide": "^0.0.56",
    "@netless/window-manager": "^0.4.0-canary.24",
    "tsup": "^5.11.11",
    "white-web-sdk": "^2.16.1"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "src/index.ts"
  }
}
