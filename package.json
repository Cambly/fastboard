{
  "name": "agora-whiteboard-sdk",
  "version": "0.0.0",
  "description": "An open source sdk based on white-web-sdk.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./src/index.tsx",
  "exports": {
    "node": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "default": "./dist/index.es.js"
  },
  "unpkg": "./dist/index.iife.js",
  "jsdelivr": "./dist/index.iife.js",
  "sideEffects": false,
  "repository": "netless-io/agora-whiteboard-web-sdk",
  "keywords": [
    "whiteboard",
    "collaboration"
  ],
  "scripts": {
    "prettier": "prettier --write .",
    "lint": "eslint --ext .ts,.tsx . && prettier --check . && tsc",
    "preinstall": "npx only-allow pnpm",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "cleanup": "rimraf ./dist",
    "dev": "vite --open"
  },
  "contributors": [
    {
      "name": "hyrious",
      "email": "hyrious@outlook.com"
    },
    {
      "name": "l1shen",
      "email": "lishen1635@gmail.com"
    }
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "white-web-sdk": "*"
  },
  "dependencies": {
    "@netless/window-manager": "^0.2.19"
  },
  "devDependencies": {
    "@hyrious/rimraf": "^0.1.0",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "classnames": "^2.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.43.4",
    "typescript": "^4.5.2",
    "vite": "^2.7.0-beta.7",
    "white-web-sdk": "^2.15.5"
  }
}
