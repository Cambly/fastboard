{
  "name": "@netless/fastboard",
  "version": "0.0.3",
  "description": "An open sourced sdk based on white-web-sdk.",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/index.es.js",
        "require": "./dist/index.cjs.js"
      },
      "default": "./dist/index.es.js",
      "types": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "esbuild-dev scripts/prepublish.ts",
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts,.tsx .",
    "dev": "vite dev --host",
    "build": "vite build",
    "build:dev": "vite build --mode development"
  },
  "contributors": [
    {
      "name": "hyrious",
      "email": "hyrious@outlook.com"
    },
    {
      "name": "l1shen",
      "email": "lishen1635@gmail.com"
    }
  ],
  "license": "MIT",
  "peerDependencies": {
    "@netless/app-countdown": "*",
    "@netless/app-geogebra": "*",
    "@netless/app-monaco": "*",
    "@netless/app-slide": "*",
    "@netless/window-manager": "*",
    "react": "*",
    "react-dom": "*",
    "white-web-sdk": "*"
  },
  "peerDependenciesMeta": {
    "@netless/app-slide": {
      "optional": true
    },
    "@netless/app-monaco": {
      "optional": true
    },
    "@netless/app-countdown": {
      "optional": true
    },
    "@netless/app-geogebra": {
      "optional": true
    }
  },
  "dependencies": {
    "@tippyjs/react": "^4.2.6",
    "clsx": "^1.1.1",
    "i18next": "^21.6.4",
    "rc-slider": "^9.7.5",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@hyrious/esbuild-dev": "^0.7.1",
    "@hyrious/rimraf": "^0.1.0",
    "@netless/app-countdown": "^0.0.1",
    "@netless/app-geogebra": "^0.0.2",
    "@netless/app-monaco": "^0.1.12",
    "@netless/app-slide": "^0.0.53",
    "@netless/window-manager": "^0.4.0-canary.0",
    "@types/node": "^17.0.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-react": "^1.1.3",
    "esbuild": "^0.14.10",
    "eslint": "^8.6.0",
    "eslint-define-config": "^1.2.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.62.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.45.2",
    "type-fest": "^2.8.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "white-web-sdk": "^2.15.14"
  }
}
