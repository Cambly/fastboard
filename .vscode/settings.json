{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.addMissingImports": true
  },
  "cSpell.words": [
    "canplay",
    "changesets",
    "dasharray",
    "fastboard",
    "geogebra",
    "pnpx",
    "preid",
    "premajor",
    "preminor",
    "prepatch",
    "sveld",
    "telebox",
    "tippyjs",
    "tsup",
    "Unsubscriber",
    "valuemax",
    "valuemin",
    "valuenow"
  ]
}
